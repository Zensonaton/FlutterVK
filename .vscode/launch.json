{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Запустить в debug-режиме",
			"request": "launch",
			"type": "dart",
			"args": [
				"--dart-define", "PRERELEASE=1",
                "--web-port=5000",
                "--web-enable-expression-evaluation"
			]
		},
		{
			"name": "Запустить в profile-режиме",
			"request": "launch",
			"type": "dart",
			"flutterMode": "profile",
			"args": [
				"--dart-define", "PRERELEASE=1"
			]
		},
		{
			"name": "Запустить в release-режиме",
			"request": "launch",
			"type": "dart",
			"flutterMode": "release",
			"args": [
				"--dart-define", "PRERELEASE=1"
			]
		}
	]
}
